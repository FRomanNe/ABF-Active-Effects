{{#> "systems/animabf/templates/common/ui/group.hbs"
  class="techniques"
}}
  {{>
  "systems/animabf/templates/common/ui/group-header.hbs"
    title=(localize "anima.ui.domine.techniques.title")
    addRowButtonData="add-technique"
  }}
  {{#> "systems/animabf/templates/common/ui/group-body.hbs"
    class="techniques"
  }}
    {{#each system.domine.techniques as |technique|}}
      <div data-item-id='{{technique._id}}' class='technique-row'>
        {{#> "systems/animabf/templates/common/ui/group.hbs"
          class="technique"
          contractible=true
          contractibleItemId=technique._id
        }}
          {{#> "systems/animabf/templates/common/ui/group-header.hbs"}}
            <div class='technique-header'>
              {{>
              "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
                class="name"
                title=(localize "anima.ui.domine.techniques.technique.header.name.title")
                inputType="text"
                inputName=(concat "system.dynamic.techniques." technique._id ".name")
                inputValue=technique.name
              }}
              <nav class="abf-tabs tabs effect-tabs-{{technique._id}} pl-4" data-group="secondary">
                <a class="item" data-tab="{{technique_id}}-main">Details</a>
                <a class="item pl-2" data-tab="effect">Effect</a>
              </nav>
            </div>
          {{/"systems/animabf/templates/common/ui/group-header.hbs"}}
          <section class="effect-body-{{technique._id}} common-group">
            <div class="tab active" data-group="secondary" data-tab="{{technique._id}}-main">
              {{#> "systems/animabf/templates/common/ui/group-body.hbs"
                class="technique"
              }}
                {{editor technique.system.enrichedDescription target=(concat "system.dynamic.techniques." technique._id ".system.description.value") button=true owner=true editable=true}}
              {{/"systems/animabf/templates/common/ui/group-body.hbs"}}
              {{#> "systems/animabf/templates/common/ui/group-footer.hbs"}}
                <div class='technique-footer'>
                  {{>
                  "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
                    class="level"
                    title=(localize "anima.ui.domine.techniques.technique.footer.level.title")
                    inputType='text'
                    inputName=(concat "system.dynamic.techniques." technique._id ".system.level.value")
                    inputValue=technique.system.level.value
                  }}
                  {{>
                  "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
                    class="strength"
                    title=(localize "anima.ui.domine.techniques.technique.footer.strength.title")
                    inputType='text'
                    inputName=(concat "system.dynamic.techniques." technique._id ".system.strength.value")
                    inputValue=technique.system.strength.value
                  }}
                  {{>
                  "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
                    class="agility"
                    title=(localize "anima.ui.domine.techniques.technique.footer.agility.title")
                    inputType='text'
                    inputName=(concat "system.dynamic.techniques." technique._id ".system.agility.value")
                    inputValue=technique.system.agility.value
                  }}
                  {{>
                  "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
                    class="dexterity"
                    title=(localize "anima.ui.domine.techniques.technique.footer.dexterity.title")
                    inputType='text'
                    inputName=(concat "system.dynamic.techniques." technique._id ".system.dexterity.value")
                    inputValue=technique.system.dexterity.value
                  }}
                  {{>
                  "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
                    class="constitution"
                    title=(localize "anima.ui.domine.techniques.technique.footer.constitution.title")
                    inputType='text'
                    inputName=(concat "system.dynamic.techniques." technique._id ".system.constitution.value")
                    inputValue=technique.system.constitution.value
                  }}
                  {{>
                  "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
                    class="willPower"
                    title=(localize "anima.ui.domine.techniques.technique.footer.willPower.title")
                    inputType='text'
                    inputName=(concat "system.dynamic.techniques." technique._id ".system.willPower.value")
                    inputValue=technique.system.willPower.value
                  }}
                  {{>
                  "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
                    class="power"
                    title=(localize "anima.ui.domine.techniques.technique.footer.power.title")
                    inputType='text'
                    inputName=(concat "system.dynamic.techniques." technique._id ".system.power.value")
                    inputValue=technique.system.power.value
                  }}
                  {{>
                  "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
                    class="martial-knowledge"
                    title=(localize "anima.ui.domine.techniques.technique.footer.martialKnowledge.title")
                    inputType='text'
                    inputName=(concat "system.dynamic.techniques." technique._id ".system.martialKnowledge.value")
                    inputValue=technique.system.martialKnowledge.value
                  }}
                </div>
              {{/"systems/animabf/templates/common/ui/group-footer.hbs"}}
            </div>
            {{> "modules/anima-active-effects/templates/effects/effects-tab.hbs" id=technique._id}}
          </section>
        {{/"systems/animabf/templates/common/ui/group.hbs"}}
      </div>
    {{/each}}
  {{/"systems/animabf/templates/common/ui/group-body.hbs"}}
{{/"systems/animabf/templates/common/ui/group.hbs"}}
